<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="crm_lead_view_form_prescription" model="ir.ui.view">
        <field name="name">CRM Form Prescription </field>
        <field name="model">crm.lead</field>
        <field name="inherit_id" ref="crm.crm_lead_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//notebook/page[@name='lead']/group/group[@class='mt48']" position="inside">
                <field name="prescription_datas" type="base64" filename="name" attrs="{'invisible':[('type','=','url')]}"/>
            </xpath>
        </field>
    </record>
</odoo>