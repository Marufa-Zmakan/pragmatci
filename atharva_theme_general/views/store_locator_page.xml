                          <a href="/page/homepage" t-attf-style="color:#{website.breadcrumb_text_color}">Home</a>
                        </li>
                        <li class="active">
                          <span t-attf-style="color:#{website.breadcrumb_text_color}">Stores</span>
                        </li>
                      </ul>
                    </div>
                  </nav>
                </t>
                <t t-if="not website.breadcum_background_image">
                  <t t-set="bread_cum" t-value="website.image_url(website,'bread_cum_image')"/>
